{
  "name": "wp-gutenberg-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "__eslintPlugin": "eslint src/",
    "__stylelintPlugin": "stylelint \"src/**/**/*.scss\" --syntax scss",
    "precommitCss": "npm run __stylelintPlugin",
    "precommitJs": "npm run __eslintPlugin",
    "precommit": "npm run precommitJs && npm run precommitCss",
    "precommitPhp": "phpcs --standard=vendor/infinum/coding-standards-wp/Infinum inc",
    "autofixPhp": "phpcbf --standard=vendor/infinum/coding-standards-wp/Infinum inc",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js"
  },
  "dependencies": {
    "@infinumjs/eslint-config": "^1.1.0",
    "@infinumjs/stylelint-config": "^1.0.0",
    "autoprefixer": "^9.1.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.4",
    "babel-preset-cgb": "^1.5.0",
    "cgb-dev-utils": "^1.4.0",
    "chalk": "^2.4.1",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.1.0",
    "eslint": "^4.15.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.6.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-wordpress": "^0.1.0",
    "filesize": "^3.5.11",
    "fs-extra": "^5.0.0",
    "gzip-size": "^4.1.0",
    "inquirer": "^5.0.0",
    "node-sass": "^4.7.2",
    "ora": "^1.3.0",
    "postcss-loader": "^3.0.0",
    "resolve-pkg": "^1.0.0",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.8.0",
    "style-loader": "^0.19.1",
    "stylelint": "^9.2.0",
    "update-notifier": "^2.3.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 Chrome versions",
              "last 2 Firefox versions",
              "last 2 Safari versions",
              "last 2 iOS versions",
              "last 1 Android version",
              "last 1 ChromeAndroid version",
              "ie 11"
            ]
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      [
        "transform-react-jsx",
        {
          "pragma": "wp.element.createElement"
        }
      ],
      [
        "@wordpress/babel-plugin-makepot",
        {
          "output": "languages/gutenberg-boilerplate.pot"
        }
      ],
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@wordpress/babel-plugin-makepot": "^2.0.3",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "uglifyjs-webpack-plugin": "^1.3.0"
  }
}
